</header>
<div id="nos-services">
    <div class="qualite">
        <div class="container">
            <div class="row">
                <div class="col-lg-5  wow fadeInRight">
                    <div class="row">
                        <h2 class="faire">
                            Faire de la qualité<br />Divertissement réalisable
                        </h2>
                    </div>

                    <div class="row regarde-app">
                        <div class="mr-3">
                            <div class="">
                                <img src="~/assets/img/qualite1.png" />
                            </div>

                        </div>
                        <div class="">
                            <h6>Divertissement en streaming</h6>
                            <p>
                                Regardez tous les canaux bein-sports,<br />
                                canal+,skysport.. et + 9000 chaines en straming
                            </p>
                        </div>
                    </div>
                    <div class="row regarde-app">
                        <div class="mr-3">
                            <div class="">
                                <img src="~/assets/img/qualite2.png" />
                            </div>

                        </div>
                        <div class="">
                            <h6>Filmes, Séries et émissions exclusives</h6>
                            <p>
                                Aviptv vous bénéficiez des nouvelles séries <br />
                                émissions, et films gratuitement.
                            </p>
                        </div>
                    </div>
                    <div class="row regarde-app">
                        <div class="mr-3">
                            <div class="">
                                <img src="~/assets/img/qualite3.png" />
                            </div>

                        </div>
                        <div class="">
                            <h6>Disponible sur android</h6>
                            <p>
                                L'application Aviptv est disponible sur google<br />
                                play store.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="telechargement" id="telechargement">
    <div class="container">
        <h2 class="faire text-center">Nos softs à télécharger</h2>

        <div class="row mt-3 ">
            @foreach (var item in Model)
            {
                <div class="col-lg-3 mt-4">
                    <div class="border-box">

                        <div class="avatar">
                            <div class="categorie-img">

                                <img src="~/assets/files/@item.Avatar" />
                            </div>
                        </div>

                        <div class="row bg-gris height-60 align-items-baseline">
                            <div class="col-lg-6 col-6">
                                <p class="name_categorie">
                                    @Html.DisplayFor(modelItem => item.name)
                                </p>
                            </div>


                            <div class="col-lg-6 col-6 voir_plus">
                                <a asp-action="afficheSubCategory" asp-route-id="@item.id">Voir Plus</a>

                            </div>
                        </div>

                    </div>

                </div>
            }
        </div>




    </div>

</div>
<div class="application1" id="application">
    <div class="container">
        <div class="row">
            <div class="col-lg-5 mt-5">
